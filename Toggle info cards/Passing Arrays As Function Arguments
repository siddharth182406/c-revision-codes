#include <stdio.h>


int func1(int array[])
{
    for(int i = 0 ; i < 4; i++)
    {
        printf("the value at %d s %d\n",i,array[i]);
    }
   // array[0] = 24; // very important point that if you change any value here,it get refeleted  in main
    
    return 0;
}


int main()
{
    int arr[] = {10,20,30,40};
    printf("The value at index 0 is %d\n",arr[0]);
    func1(arr);
    printf("The value at index 0 is %d\n",arr[0]);
    
    return 0;
}
