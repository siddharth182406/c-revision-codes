#include <stdio.h> // in this code we are taking inputes from user and then printing 

int main() // using for loop
{
   int marks[5],i;
   
   for(i = 0; i < 4; i++){
       
       printf("Entre the value of %d element of the arry\n",i);
       scanf("%d", &marks[i]);
   }
   
   for(i = 0; i < 4; i++){
       
       printf("The value of %d element of the arry is %d\n",i,marks[i]);
      
   }

    return 0;
}

// writing normal without using for loop

#include <stdio.h>

int main()
{
   int marks[5],i;
   
   marks[0] = 10;
   marks[1] = 20;
   marks[2] = 30;
   marks[3] = 40;
   marks[4] = 50;
   
   printf("marks of student 0 is %d\n",marks[0]);
   printf("marks of student 1 is %d\n",marks[1]);
   printf("marks of student 2 is %d\n",marks[2]);
   printf("marks of student 3 is %d\n",marks[3]);
   printf("marks of student 4 is %d\n",marks[4]);
    return 0;
}
